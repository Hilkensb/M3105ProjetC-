#test pour
#toutes le valeurs de test doivent valoire 5 après éxecution
procedure principale()
    test1=0;
    test2=0;
#test1
    pour(i=0;i<5;i=i+1)
    test1=test1+1;
    finpour
#test2
    i=0;
    pour(;i<5;i=i+1)
    test2=test2+1;
    finpour
#test3
    pour(test3=0;test3<5;)
    test3=test3+1;
    finpour
finproc

